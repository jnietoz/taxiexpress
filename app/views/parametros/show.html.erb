<%- model_class = Parametro -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:texto) %>:</strong></dt>
  <dd><%= @parametro.texto %></dd>
  <dt><strong><%= model_class.human_attribute_name(:valor) %>:</strong></dt>
  <dd><%= @parametro.valor %></dd>
  <dt><strong><%= model_class.human_attribute_name(:entidad_id) %>:</strong></dt>
  <dd><%= @parametro.entidad_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              entidad_parametros_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_entidad_parametro_path(@parametro.entidad.id,@parametro), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              entidad_parametro_path(@parametro.entidad.id,@parametro),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
